<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Xincgc -Xms512M -Xmx1024M -classpath %classpath org.bukkit.craftbukkit.Main -P ../../target --nojline</exec.args>
                <exec.executable>java</exec.executable>
                <exec.workingdir>C:\Users\Goblom\Documents\NetBeansProjects\TenJava\src\test\server\</exec.workingdir>
            </properties>
        </action>
        <action>
            <actionName>debug</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -Xincgc -Xms512M -Xmx1024M -classpath %classpath org.bukkit.craftbukkit.Main -P ../../target --nojline</exec.args>
                <exec.executable>java</exec.executable>
                <jpda.listen>true</jpda.listen>
                <exec.workingdir>C:\Users\Goblom\Documents\NetBeansProjects\TenJava\src\test\server\</exec.workingdir>
            </properties>
        </action>
        <action>
            <actionName>profile</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>${profiler.args} -Xincgc -Xms512M -Xmx1024M -classpath %classpath org.bukkit.craftbukkit.Main -P ../../target --nojline</exec.args>
                <exec.executable>${profiler.java}</exec.executable>
                <exec.workingdir>C:\Users\Goblom\Documents\NetBeansProjects\TenJava\src\test\server\</exec.workingdir>
            </properties>
        </action>
    </actions>
